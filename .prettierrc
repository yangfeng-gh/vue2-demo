{
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    }
  ],
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": false,
  "trailingComma": "none", // 是否使用尾逗号，有三个可选值"<none|es5|all>"
  "bracketSpacing": true,
  "parser": "babel" // 代码的解析引擎，默认为babylon，与babel相同。
}
